{# {% set topBannerImage = top_banner.global_image.global_image %} #}

{{ navMenu(navMenuLink) }}
<section class="group-section top-section">
    <section class="segment-outer relative">
        <figure class="segment-inner">
            {{global_image(pageCMS['top-banner'].global_image, classes='-z-1 h-screen w-full image_frame sm:3:4 lg:3:4 2xl:16:9 4xl:9:16')}}
        </figure>
    </section>
    {% for event in data.groupData %}
        <section class="group-container">
            {{ groupVenues(event) }}
        </section>
    {% endfor %}
</section>
