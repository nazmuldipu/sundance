<section class="segment-outer bg-sn-5 px-4">
    <section class="segment-inner">
        {{bridge(bridge_slider)}}
    </section>
</section>
<section class="segment-outer bg-sn-5 px-4">
    <section class="segment-inner">
        <section class="details__middle-banner">
            {% for item in articles %}
                {% set object = item.simple_article %}
                <article class="dining-details__figcaption">
                    <header>
                        <h3 class="dining-details__middle-banner--header font-ivar bold text-center">
                            {{ object.title }}
                        </h3>
                    </header>
                    <div class="dining-details__middle-banner--descriptions font-ivar">
                        {{ object.description | safe }}
                    </div>
                </article>
            {% endfor %}
        </section>
    </section>
</section>
<section class="segment-outer">
    <section class="segment-inner">
        <article class="details__quote bg-sn-4 font-ivar">
            <div class="dining-details__middle-banner--descriptions font-ivar">
                {{ notice.description | safe }}
            </div>
        </article>
        <div class="dining-details__middle-banner--descriptions font-ivar text-center">
            {{ contactInfo.description | safe }}
        </div>
    </section>
</section>
