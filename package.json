{
  "name": "skipper-site-template",
  "version": "1.0.0",
  "description": "Skipper Hospitality Website Template",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "cross-env NODE_ENV=production node tooling/build.js.js",
    "watch:js": "node tooling/dev.js.js",
    "build:html": "cross-env NODE_ENV=production npx @11ty/eleventy --config=.eleventy.cjs",
    "watch:css": "node tooling/dev.css.js",
    "build:css": "cross-env NODE_ENV=production node tooling/build.css.js",
    "build-dev": "cross-env NODE_ENV=production THE_ENV=live ACTIVE_API_URI=DEV node tooling/build.js",
    "build-stage": "cross-env NODE_ENV=production THE_ENV=live ACTIVE_API_URI=STAGE node tooling/build.js",
    "build-prod": "cross-env NODE_ENV=production THE_ENV=live ACTIVE_API_URI=PROD node tooling/build.js",
    "watch:html": "cross-env NODE_ENV=development npx @11ty/eleventy --config=.eleventy.cjs",
    "watch-dev": "cross-env NODE_ENV=development THE_ENV=local ACTIVE_API_URI=DEV node tooling/dev.js",
    "watch-stage": "cross-env NODE_ENV=development THE_ENV=local ACTIVE_API_URI=STAGE node tooling/dev.js",
    "watch-prod": "cross-env NODE_ENV=development THE_ENV=local ACTIVE_API_URI=PROD node tooling/dev.js",
    "resize-images": "node tooling/images.js",
    "snapshots": "percy exec -- node snapshots.js",
    "cli": "cross-env NODE_ENV=production node tooling/cli"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "author": "Skipper Hospitality",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@apollo/client": "^3.4.16",
    "@percy/script": "^1.1.0",
    "@skipperhospitality/webcomponents": "^1.0.0",
    "autoprefixer": "^10.2.3",
    "browser-sync": "^2.26.14",
    "chokidar": "^3.5.1",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "cssnano": "^4.1.10",
    "directory-tree": "^2.2.9",
    "esbuild": "^0.8.28",
    "graphql": "^15.7.2",
    "html-minifier": "^4.0.0",
    "http-server": "^0.12.3",
    "just-compose": "^2.0.0",
    "p-all": "^4.0.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-nesting": "^7.0.1",
    "sharp": "^0.28.2",
    "tailwindcss": "^2.1.0"
  }
}
